// Declare some variables
int a = 5;
float b = 3.0;

// Simple if-else statement:
if ( a < 10 ) {
    print("a is less than 10");
} else {
    print("a is greater than or equal to 10");
}

// An if statement without an else:
if ( b > 2.5 ) {
    print(b);
}

// A loop that runs 3 times:
loop 3 {
    print(a);
}
