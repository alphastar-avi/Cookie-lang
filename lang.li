int a = 5;
int b = 10;

// Using symbols:
if (a < b) {
    print("Using <: a is less than b");
}

if (b >= a) {
    print("Using >=: b is greater than or equal to a");
}

if (a == 5) {
    print("Using ==: a equals 5");
}

// Using keywords:
if (a less than b) {
    print("Using 'less than': a is less than b");
}

if (b greater than equals a) {
    print("Using 'greater than equals': b is greater than or equal to a");
}

if (a equals 5) {
    print("Using 'equals': a equals 5");
}
