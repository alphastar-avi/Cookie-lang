{
  "name": "langli-extension",
  "displayName": "Cookie Language Extension",
  "description": "A custom VS Code extension for the cookie programming language with syntax highlighting, error detection, and code snippets. Crafted with detication!",
  "version": "6.0.0",
  "publisher": "alph",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphastar-avi/Goofy-Compiler"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:langli",
    "onCommand:langli.toggleGoofyHighlights",
    "onCommand:Goofy.dorito"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "langli.toggleGoofyHighlights",
        "title": "Toggle Goofy Highlights"
      },
      {
        "command": "Goofy.dorito",
        "title": "Who's Dorito"
      }
    ],
    "languages": [
      {
        "id": "langli",
        "aliases": [
          "Lang.li",
          "langli"
        ],
        "extensions": [
          ".cook"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "langli",
        "scopeName": "source.langli",
        "path": "./syntaxes/langli.tmLanguage.json"
      }
    ]
  },
  "snippets": [
    {
      "language": "langli",
      "path": "./snippets/langli.json"
    }
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.97.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "langli-extension": "file:"
  }
}
